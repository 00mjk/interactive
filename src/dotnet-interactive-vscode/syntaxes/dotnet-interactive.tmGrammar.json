{
    "scopeName": "source.dotnet-interactive",
    "patterns": [
        {
            "begin": "(?!(#!))(?i)c(#|s(harp)?)",
            "end": "#!",
            "patterns": [
                {
                    "include": "source.cs"
                }
            ]
        },
        {
            "begin": "(?!(#!))(?i)f(#|s(harp)?)",
            "end": "#!",
            "patterns": [
                {
                    "include": "source.fsharp"
                }
            ]
        },
        {
            "begin": "(?!(#!))(?i)powershell|pwsh",
            "end": "#!",
            "patterns": [
                {
                    "include": "source.powershell"
                }
            ]
        },
        {
            "begin": "(?!(#!))(?i)html",
            "end": "#!",
            "patterns": [
                {
                    "include": "text.html.basic"
                }
            ]
        },
        {
            "begin": "(?!(#!))(?i)javascript|js",
            "end": "#!",
            "patterns": [
                {
                    "include": "source.js"
                }
            ]
        },
        {
            "begin": "(?!(#!))(?i)markdown|md",
            "end": "#!",
            "patterns": [
                {
                    "include": "text.html.markdown"
                }
            ]
        }
    ]
}
