{
  "name": "dotnet-interactive-vscode",
  "displayName": "DotNet Interactive Notebooks",
  "description": "DotNet Interactive Notebooks for VS Code",
  "enableProposedApi": true,
  "preview": true,
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.44.0"
  },
  "repository": {
    "url": "https://github.com/dotnet/interactive"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dotnet-interactive",
    "onNotebookEditor:dotnet-interactive"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "dotnet-interactive",
        "aliases": [
          "DotNet Interactive"
        ],
        "filenamePatterns": [
          "*.dotnet-interactive"
        ]
      }
    ],
    "notebookProvider": [
      {
        "viewType": "dotnet-interactive",
        "displayName": "DotNet Interactive Notebook",
        "selector": [
          {
            "filenamePattern": "*.dotnet-interactive"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "dotnet-interactive-vscode.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "integration-test": "node ./out/tests/integration/runTest.js",
    "pretest": "npm run compile && npm run lint",
    "test": "mocha --opts mocha.opts",
    "ci": "npm test -- --reporter mocha-multi-reporters --reporter-options configFile=testConfig.json",
    "tdd": "npm test -- --watch"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "chai": "4.2.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "typings": "^2.1.1",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "rxjs": "^6.5.5"
  }
}
